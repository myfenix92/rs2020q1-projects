{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  // "parser": "babel-eslint",
  "extends": ["airbnb/base"],
  "rules": {
    "linebreak-style": [
      "error",
      "windows"
  ],
    "semi": "off",
    "quotes": "off",
    "no-console": "off",
    "max-len": "off",
    "no-plusplus": "off",
    "no-use-before-define": "off",
    "no-useless-escape": "off"
  }
}